{
  "name": "duplicate-tab-detector",
  "version": "1.0.0",
  "description": "React hook that detects duplicated browser tabs and resets per-tab session ids.",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "prepare": "npm run build",
    "check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.2.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "react",
    "hook",
    "sessionstorage",
    "duplicate-tab",
    "browser",
    "localstorage"
  ]
}
